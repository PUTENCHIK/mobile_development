<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BlankFragment3">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:gravity="center">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/title_margin_bottom"
            android:text="@string/title_frag3"
            android:textSize="@dimen/title_text_size" />

        <Button
            android:id="@+id/button_to_frag5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btn_to_frag5"
            android:textSize="@dimen/button_text_size" />

        <Button
            android:id="@+id/button_to_frag6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btn_to_frag6"
            android:textSize="@dimen/button_text_size" />

        <include
            layout="@layout/button_back"
            android:id="@+id/button_back"/>

    </LinearLayout>

</FrameLayout>